<!DOCTYPE html>
<html>
  <head>
    <title>Login</title>
  </head>
  <body>
    <h1>Login</h1>
    <form action="/loginsucc" method="POST">
      <label for="email">Email:</label>
      <input
        type="email"
        id="email"
        name="email"
        required
        value="riswan14.rm@gmail.com"
      /><br /><br />

      <label for="password">Password:</label>
      <input
        type="password"
        id="password"
        name="password"
        required
        onblur="validation()"
      /><br /><br />
      <p style="color: red" id="passwordError"></p>

      <input type="submit" value="Login" />
    </form>
    <script src="../validation/validation.js"></script>
  </body>
</html>
